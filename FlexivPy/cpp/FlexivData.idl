module FlexivMsg {
    struct FlexivCmd {
        double tau_ff[7];
        double q[7];
        double dq[7];
        double kp[7];
        double kv[7];
        int32 mode;
        string g_cmd;
        string timestamp;
    };

    struct FlexivState {
        double q[7];
        double dq[7];
        double tau[7];
        double ft_sensor[6];
        string timestamp;
        string g_state;
        boolean g_moving;
        double g_force;
        double g_width;
    };

    struct FlexivImage {
        sequence<octet> data;
        string timestamp;
    };

    typedef double Array7[7];

    struct FlexivObjects {
        sequence<string> names;
        sequence<Array7> poses;
    };


};
